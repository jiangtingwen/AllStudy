<view class="trends">
  <trend repo="{{ item }}" wx:for="{{ trends }}" 
  wx:key="{{ index }}">
  </trend>
  <picker mode="multiSelector" value="{{selectedIndices}}" bindchange="changeFilter" class="lang-picker"
  range="{{selectorValues}}" range-key="label"
  >
  <button type="primary" size="mini">{{since.label}} / {{lang.label}}</button>
  </picker>
</view>